<html>
<head>
    <style type="text/css">
        body {
            margin: 10px;
        }
        img {
            width: 25px;
        }
        <%- include('header_css') %>
    </style>
</head>
<body>

<%- include('header', { current: 'tiny' }) %>

<%_ for (const year of Object.keys(images).sort().reverse()) { _%><%#
    %><%_ const issues = images[year]; _%><%#

    %><%_ for (const ymd of Object.keys(issues).sort()) { _%><%#
        %><%_ const issue = issues[ymd]; _%><%#

        %><a href="<%= issue.urls.LARGE %>"><%#
            %><img src="<%= issue.urls.THUMBNAIL %>" title="<%= issue.displayDate %> issue" /><%#
        %></a><%#

    %><%_ } _%><%#

%><%_ } _%>

</body>
</html>

