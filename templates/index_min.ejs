<html>
<head>
    <style type="text/css">
        body {
            margin: 10px;
        }
    </style>
</head>
<body>

<%_ for (const year of Object.keys(images).sort().reverse()) { _%>
    <%_ const issues = images[year]; _%>

    <%_ for (const ymd of Object.keys(issues).sort()) { _%>
        <%_ const issue = issues[ymd]; _%>

        <a href="<%= issue.files.LARGE %>"><%#
            %><img src="<%= issue.files.THUMBNAIL %>" title="<%= issue.displayDate %> issue" /><%#
        %></a>

    <%_ } _%>

<%_ } _%>

</body>
</html>

